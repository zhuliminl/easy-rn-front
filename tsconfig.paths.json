{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@ui/*": [
        "src/components/UI/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@cons/*": [
        "src/constants/*"
      ],
      "@environment/*": [
        "src/environments/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@pages/Common/*": [
        "src/pages/Common/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ]
    }
  }
}